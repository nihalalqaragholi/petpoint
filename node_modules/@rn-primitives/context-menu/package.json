{
  "name": "@rn-primitives/context-menu",
  "version": "1.2.0",
  "description": "Primitive context menu",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./dist/context-menu": {
      "import": "./dist/context-menu.mjs",
      "require": "./dist/context-menu.js",
      "types": "./dist/context-menu.d.ts",
      "default": "./dist/context-menu.js"
    }
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@radix-ui/react-context-menu": "^2.2.15",
    "@rn-primitives/hooks": "1.3.0",
    "@rn-primitives/slot": "1.2.0",
    "@rn-primitives/utils": "1.2.0",
    "@rn-primitives/types": "1.2.0"
  },
  "devDependencies": {
    "@tsconfig/react-native": "^1.0.1",
    "@types/react": "~19.0.14",
    "react": "19.0.0",
    "react-native": "0.79.2",
    "tsup": "^8.1.0",
    "@rn-primitives/portal": "1.3.0"
  },
  "peerDependencies": {
    "@rn-primitives/portal": "*",
    "react": "*",
    "react-native": "*",
    "react-native-web": "*"
  },
  "peerDependenciesMeta": {
    "react-native": {
      "optional": true
    },
    "react-native-web": {
      "optional": true
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist",
    "pub:beta": "pnpm publish --no-git-checks --access public --tag beta",
    "pub:next": "pnpm publish --no-git-checks --access public --tag next",
    "pub:release": "pnpm publish --access public"
  }
}